{
    "manifest_version": 2,
    "name": "Instagram Grabber",
    "version": "1.0.0",
    "description": "Get photos from instagram. Use at your own risk.",
    "short_name": "IG Grabber",
    "permissions": ["activeTab", "https://instagram.com/p/*", "downloads"],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
